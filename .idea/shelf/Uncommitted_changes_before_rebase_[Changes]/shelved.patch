<+>//pagina inicial do site\r\n<html>\r\n<head>\r\n<meta charset=\"utf-8\">\r\n<title>aula 1</title>\r\n</head>\r\n<body>\r\n    <script>\r\n        //função fatorial em javascript\r\nfunction factorial(num) {\r\n    let result = 1;\r\n    for (let i = 1; i <= num; i++) {\r\n      result *= i;\r\n    }\r\n    return result;\r\n  }\r\n\r\n    console.log(factorial(3));\r\n\r\n\r\n// Path: fatorial.js recursivo\r\n//function factorial(num) {\r\n//    if (num === 0) {\r\n//        return 1;\r\n//    }\r\n//    return num * factorial(num - 1);\r\n//}\r\n\r\n//console.log(factorial(5));\r\n\r\n//função que entra com altura e sexo de 5 pessoas e retorna quantas são mulheres e quantos são homens, a maior altura e se é homem ou mulher\r\n\r\nfunction analyzePeople(people) {\r\n    let maleCount = 0;\r\n    let femaleCount = 0;\r\n    let tallestPerson = { height: 0, gender: \"\" };\r\n    for (let i = 0; i < people.length; i++) {\r\n        if (people[i].gender === \"male\") {\r\n            maleCount++;\r\n        } else {\r\n            femaleCount++;\r\n        }\r\n        if (people[i].height > tallestPerson.height) {\r\n            tallestPerson = people[i];\r\n        }\r\n    }\r\n    return {\r\n        maleCount: maleCount,\r\n        femaleCount: femaleCount,\r\n        tallestPerson: tallestPerson\r\n    };\r\n}\r\n\r\nlet people = [\r\n    { height: 170, gender: \"male\" },\r\n    { height: 160, gender: \"female\" },\r\n    { height: 165, gender: \"male\" },\r\n    { height: 170, gender: \"female\" },\r\n    { height: 175, gender: \"male\" }\r\n];\r\n\r\nlet result = analyzePeople(people);\r\nconsole.log(result);\r\n\r\n//função que retorna um array com a quantidade de números primos até o número passado como parâmetro\r\nfunction primos(quantidade) {\r\n    let numeros = [];\r\n    let numero = 2;\r\n    while (numeros.length < quantidade) {\r\n        let primo = true;\r\n        for (let i = 2; i < numero; i++) {\r\n            if (numero % i === 0) {\r\n                primo = false;\r\n                break;\r\n            }\r\n        }\r\n        if (primo) {\r\n            numeros.push(numero);\r\n        }\r\n        numero++;\r\n    }\r\n    return numeros;\r\n}\r\nconsole.log(primos(4));\r\n//função que recebe 3 lados de um triangulo e retorna o tipo de triangulo\r\nfunction tipoTriangulo(a, b, c) {\r\n    if (a == b && b == c) {\r\n        return \"Equilátero\";\r\n    } else if (a == b || b == c || a == c) {\r\n        return \"Isósceles\";\r\n    } else {\r\n        return \"Escaleno\";\r\n    }\r\n}\r\n\r\nconsole.log(tipoTriangulo(3, 5, 9));\r\n    </script>\r\n<h1>aula 1</h1>\r\n\r\n</body>\r\n</html>
